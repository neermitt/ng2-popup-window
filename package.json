    "lint": "yarn run tslint --project tsconfig.json \"src/**/*.ts\"",
    "preclean:install": "yarn run clean",
    "rimraf": "rimraf",
    "standard": "standard index.js",
    "tslint": "tslint",
    "webpack": "webpack",
    "test": "karma start"
  },
  "peerDependencies": {
    "@angular/common": "2.x || 4.x",
    "@angular/core": "2.x || 4.x"
  },
